<!--pages/main/index.wxml-->
<view class="container">
  <swiper  class="swiper_box" indicator-dots='true' indicator-color='#00000' autoplay="{{autoplay}}"  interval="{{interval}}"
  duration="{{duration}}" >
  <block wx:for="{{imgUrls}}" wx:key="id">
    <swiper-item>
      <image data-id="{{item.businessId}}"
       src="{{item}}" class="slide-image" width="100%" height="400rpx"  mode='widthFix'/>   
    </swiper-item>
  </block>
  </swiper>
  <text>关注微信公众号：丈亭杨梅</text>
</view>
